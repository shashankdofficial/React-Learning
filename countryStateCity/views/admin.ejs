<%- include('include/header'); -%>

    <!-- Main Start -->
    <main class="mt-5 pt-3">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <h4>Dashboard</h4>
                </div>
            </div>

            <!-- Table Start -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12 mb-3">
                        <div class="card">
                            <div class="card-header">
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="example" class="table table-striped">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>Email</th>
                                                <th>Department</th>
                                                <th>Role</th>
                                                <th>Date of Birth</th>
                                                <th>Date of Joining</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        </tbody>
                                    </table>
                                    <!-- <div id="pagination"></div> -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Table End -->

            <!-- Country State City -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col-4">
                        <label for="Coutry" class="form-group">Country</label>
                        <!-- <input class="form-control" id="country" type="text"> -->
                        <select class="form-control" name="Country" id="country">

                        </select>
                    </div>
                    <div class="col-4">
                        <label for="Coutry" class="form-group">State</label>
                        <!-- <input class="form-control" id="state" type="text"> -->
                        <select class="form-control" name="State" id="state">

                        </select>
                    </div>
                    <div class="col-4">
                        <label for="Coutry" class="form-group">City</label>
                        <!-- <input class="form-control" id="city" type="text"> -->
                        <select class="form-control" name="City" id="city">

                        </select>
                    </div>
                </div>
            </div>
            <!-- Country State City -->
    </main>
    <!-- Main End -->

